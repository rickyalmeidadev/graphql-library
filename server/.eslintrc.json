{
    "env": {
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "no-use-before-define": "off",
        "no-unused-vars": "off",
        "no-console": "off"
    }
}
